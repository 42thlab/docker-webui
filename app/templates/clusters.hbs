<div>
  Clusters count: {{clustersCount}}

  {{#each cluster in model}}
  <div class='jumbotron'>
    <div class='row'>
      {{#link-to 'cluster' cluster}}
      <div class='col-md-2'>{{cluster.name}}</div>
      {{/link-to}}

      {{#each node in cluster.nodes}}
      {{#link-to 'node' node}}
      <div class='col-md-2'>{{node.name}}</div>
      {{/link-to}}
      {{/each}}
    </div>
  </div>
  {{/each}}
</div>
